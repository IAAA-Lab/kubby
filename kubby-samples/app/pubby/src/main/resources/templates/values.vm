## Renders the values of a single ResourceProperties, given in $property, as an unordered list
<ul class="property-values">
    #foreach ($value in $property.SimpleValues)
        <li>
            #if ($value.isURI)
                #if ($showLabels && $value.label)
                    <a class="uri" href="$value.BrowsableURL" title="$value.URI">$value.label</a>
                #elseif ($value.hasPrefix)
                    <a class="uri" href="$value.BrowsableURL" title="$value.URI">
                        <small>$value.Prefix:</small>$value.LocalName</a>
                #else
                    <small>&lt;</small>
                    <a class="uri" href="$value.BrowsableURL">$value.URI</a>
                    <small>&gt;</small>
                #end
            #elseif ($value.isLiteral)
                <span class="literal">$value.LiteralLexicalForm
                    #if ($value.DatatypeLabel)
                        <small> ($value.DatatypeLabel)</small>
                    #end
                    #if ($value.LiteralLanguage != "")
                        <small> ($value.LiteralLanguage)</small>
                    #end
    </span>
            #end
        </li>
    #end
</ul>
